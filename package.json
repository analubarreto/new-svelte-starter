    "postcss-import": "^14.0.0",
    "sirv-cli": "^1.0.0",
    "svelte-i18n": "^3.3.6",
    "svelte-icons": "^2.1.0",
    "svelte-routing": "^1.5.0",
    "workbox-sw": "^6.1.1"
  },
  "resolutions": {
    "postcss": "8.1.7"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "svelte-jester"
